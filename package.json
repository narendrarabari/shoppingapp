{
     "name": "shopingapp",
     "version": "0.0.1",
     "description": "shopping app",
     "main": "/lib/main.js",
     "scripts": {
    "build": "./node_modules/babel-cli/bin/babel.js src --out-dir lib --plugins transform-react-jsx --presets es2015",
    "bundle": "./node_modules/browserify/bin/cmd.js lib/client.js -o public/js/bundle.js",
    "start": "npm run build && npm run bundle && node lib/main.js"
    },
     "dependencies": {
         "body-parser": "^1.15.2",
         "express": "^4.14.0",
         "multer": "^1.2.0",
          "ejs": "^2.3.4",
        "react": "^0.14.2",
        "react-dom": "^0.14.2"
     },
    "devDependencies": {
        "browserify": "^12.0.1",
        "babel-cli": "^6.1.2",
        "babel-plugin-transform-react-jsx": "^6.0.18",
        "babel-preset-es2015": "^6.1.2"
     },
     "author": {
         "name": "narendra"
     }
 }